check-am: 

check: 
	(cd SMP_Check; make; ./SMP_Check)

install: 
	@echo "Making install in `$(PWD)`"
	@for i in $(SUBDIRS) ; do \
		echo " Entering subdirectory $$I"; \
		( cd $$i; $(MAKE) $(MFLAGS) install ) ; \
	done

clean: 
	@echo "Making clean in `$(PWD)`"
	@for i in $(SUBDIRS) ; do \
		echo " Entering subdirectory $$I"; \
		( cd $$i; $(MAKE) $(MFLAGS) clean ) ; \
	done
	@echo "Cleaning $(SMPDISTDIR)"
	@(cd $(SMPDISTDIR)/lib; for files in *.a ; do \
		if test -f $$files ; then \
		  echo "Removing $$files"; \
		  rm -f $$files; \
		fi; \
		done )
	@(cd $(SMPDISTDIR)/lib; for files in *.@SHEXT@ ; do \
		if test -f $$files ; then \
		  echo "Removing $$files"; \
		  rm -f $$files; \
		fi; \
		done )
	@(cd $(SMPDISTDIR)/include/smp/Modules; for files in *.asn1 ; do \
		if test -f $$files ; then \
		  echo "Removing $$files"; \
		  rm -f $$files; \
		fi; \
		done )
	@(cd $(SMPDISTDIR)/include/smp; for files in *.h ; do \
		if test -f $$files ; then \
		  echo "Removing $$files"; \
		  rm -f $$files; \
		fi; \
		done )


distclean:
	@(list='$(DISTROCLEAN)'; \
	for files in $$list; do \
	 if test -f $$files ; then \
	   echo "Removing $$files"; \
	   rm -f $$files; \
         fi; \
	done )

 .PHONY: all-redirect


# Tell versions [3.59,3.63) of GNU make to not export all variables.
